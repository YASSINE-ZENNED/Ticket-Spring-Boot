{
  "$schema": "https://www.krakend.io/schema/v2.5/krakend.json",
  "version": 3,
  "echo_endpoint": true,
  "host": ["http://localhost:8082"],
  "extra_config": {
    "server/virtualhost": {
      "hosts": ["host-a.tld:1234","host-a.tld"]
    }
  },
  "endpoints": [
    {
      "endpoint": "/test",
      "backend": [{
        "url_pattern": "/evets/AllEvents",
        "allow": ["req_uri"]
      }]
    }
  ]
}
